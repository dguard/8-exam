block('user')(

    content()(function() {
        return [
            {
                elem: 'icon ' + this.ctx.type,
                content: this.ctx.icon
            },
            {
                elem: 'name',
                content: this.ctx.name
            },
            {
                elem: 'text',
                content: this.ctx.text
            }
        ];
    })

);